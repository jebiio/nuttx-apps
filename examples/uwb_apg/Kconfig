#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXAMPLES_UWB_APG
	tristate "\"uwbapg, World!\" example"
	default n
	---help---
		Enable the \"uwbapg, World!\" example

if EXAMPLES_UWB_APG

config EXAMPLES_UWB_APG_PROGNAME
	string "Program name"
	default "uwb_apg"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config EXAMPLES_UWB_APG_PRIORITY
	int "uwb_apg task priority"
	default 100

config EXAMPLES_UWB_APG_STACKSIZE
	int "uwb_apg stack size"
	default DEFAULT_TASK_STACKSIZE

endif
