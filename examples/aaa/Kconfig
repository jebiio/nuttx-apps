#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXAMPLES_AAA
	tristate "\"Aaa, World!\" example"
	default n
	---help---
		Enable the \"Aaa, World!\" example

if EXAMPLES_AAA

config EXAMPLES_AAA_PROGNAME
	string "Program name"
	default "AAA"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config EXAMPLES_AAA_PRIORITY
	int "Aaa task priority"
	default 100

config EXAMPLES_AAA_STACKSIZE
	int "Aaa stack size"
	default DEFAULT_TASK_STACKSIZE

endif
